<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">
	<excludes>
		<pattern>/css/*</pattern>
		<pattern>/image/*</pattern>
		<pattern>/js/*</pattern>
		<pattern>/userdata/*</pattern>
		<pattern>/login.jsp</pattern>
	</excludes>
	
	<decorator name="public" page="public.jsp">
		<pattern>*Public.spr</pattern>
		<pattern>*index.spr</pattern>
	</decorator>
	
	<decorator name="default" page="default.jsp">
		<pattern>*Create.jsp</pattern>
		<pattern>*Detail.jsp</pattern>
		<pattern>*Modify.jsp</pattern>
		<pattern>*Search.jsp</pattern>
		<pattern>/userIndex.jsp</pattern>
		<pattern>/oprst.jsp</pattern>
		<pattern>*home.spr</pattern>
		<pattern>*find.spr</pattern>
		<pattern>*search.spr</pattern>
		<pattern>*tomodify.spr</pattern>
	</decorator>
	
</decorators>
